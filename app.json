{
  "expo": {
    "name": "SafeWoman-Front",
    "slug": "SafeWoman-Front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "safewomanfront",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "SafeWoman necesita acceso a Bluetooth para conectarse con tu pulsera de emergencia y recibir alertas.",
        "NSBluetoothPeripheralUsageDescription": "SafeWoman necesita acceso a Bluetooth para conectarse con tu pulsera de emergencia y recibir alertas.",
        "NSLocationWhenInUseUsageDescription": "SafeWoman necesita tu ubicación para enviarla en caso de emergencia.",
        "NSLocationAlwaysUsageDescription": "SafeWoman necesita tu ubicación para enviarla en caso de emergencia."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.BLUETOOTH_ADVERTISE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "package": "com.lokotedelamuete.SafeWomanFront"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-ble-plx",
        {
          "isBackgroundEnabled": false,
          "modes": ["central"],
          "bluetoothAlwaysPermission": "SafeWoman necesita acceso a Bluetooth para conectarse con tu pulsera de emergencia."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "minSdkVersion": 21
          }
        }
      ],
      "expo-location"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5f63b5bc-8e63-4187-bd69-832954dcecb3"
      }
    },
    "owner": "lokotedelamuete"
  }
}